Cell[BoxData[
 RowBox[{
  RowBox[{"Imoment", "=", 
   RowBox[{
    SubsuperscriptBox["\[Integral]", "0", "a"], 
    RowBox[{
     SubsuperscriptBox["\[Integral]", 
      RowBox[{
       RowBox[{"-", "a"}], "/", "2"}], 
      RowBox[{"a", "/", "2"}]], 
     RowBox[{
      SubsuperscriptBox["\[Integral]", "0", 
       RowBox[{"-", "a"}]], 
      RowBox[{
       RowBox[{"(", GridBox[{
          {
           RowBox[{
            SuperscriptBox["y", "2"], "+", 
            SuperscriptBox["z", "2"]}], 
           RowBox[{
            RowBox[{"-", "x"}], "*", "y"}], 
           RowBox[{
            RowBox[{"-", "x"}], "*", "z"}]},
          {
           RowBox[{
            RowBox[{"-", "y"}], "*", "x"}], 
           RowBox[{
            SuperscriptBox["x", "2"], "+", 
            SuperscriptBox["z", "2"]}], 
           RowBox[{
            RowBox[{"-", "y"}], "*", "z"}]},
          {
           RowBox[{
            RowBox[{"-", "z"}], "*", "x"}], 
           RowBox[{
            RowBox[{"-", "z"}], "*", "y"}], 
           RowBox[{
            SuperscriptBox["x", "2"], "+", 
            SuperscriptBox["y", "2"]}]}
         }], ")"}], 
       RowBox[{"\[DifferentialD]", "x"}], 
       RowBox[{"\[DifferentialD]", "y"}], 
       RowBox[{"\[DifferentialD]", "z"}]}]}]}]}]}], ";"}]], "Input",
